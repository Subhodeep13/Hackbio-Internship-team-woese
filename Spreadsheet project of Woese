def hamming_distance (lhs,rhs):
    return len([(x,y) for x,y in zip(lhs,rhs) if x is y])

slack_name = 'Subhodeep'
twitter_username= 'Subho15376402'

print(hamming_distance(slack_name,twitter_username))

